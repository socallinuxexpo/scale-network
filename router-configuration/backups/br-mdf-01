            2001:470:f026:103::/64;
            2001:470:f026:503::/64;
        }
    }
}
policy-options {
    prefix-list lan-aggregate-v4 {
        10.0.0.0/8;
    }
    prefix-list lan-aggregate-v6 {
        2001:470:f026::/48;
    }
    policy-statement DEFAULT-ORIGINATE-v4 {
        from {
            protocol static;
            route-filter 0.0.0.0/0 exact;
        }
        then accept;
    }
    policy-statement DEFAULT-ORIGINATE-v6 {
        from {
            route-filter ::/0 exact;
        }
        then accept;
    }
}
firewall {
    family inet {
        filter b2b-comms-v4 {
            term b2b-pkt {
                from {
                    source-prefix-list {
                        lan-aggregate-v4;
                    }
                    destination-prefix-list {
                        lan-aggregate-v4;
                    }
                }
                then {
                    packet-mode;
                    accept;
                }
            }
            term allow-all {
                then accept;
            }
        }
        filter from-internet4 {
            term v6-tun-pkt {
                from {
                    protocol 41;
                }
                then packet-mode;
            }
            term drop-ssh {
                from {
                    protocol tcp;
                    destination-port ssh;
                    interface irb.999;
                }
                then {
                    discard;
                }
            }
            term default-allow {
                then accept;
            }
        }
        filter to-internet4 {
            term v6-tun-pkt {
                from {
                    protocol 41;
                }
                then packet-mode;
            }
            term fuck-quic {
                from {
                    protocol udp;
                    destination-port [ http https ];
                }
                then {
                    reject;
                }
            }
            term default {
                then accept;
            }
        }
    }
    family inet6 {
        filter b2b-comms-v6 {
            term b2b-pkt {
                from {
                    source-prefix-list {
                        lan-aggregate-v6;
                    }
                    destination-prefix-list {
                        lan-aggregate-v6;
                    }
                }
                then {
                    packet-mode;
                    accept;
                }
            }
            term allow-all {
                then accept;
            }
        }
        filter to-internet6 {
            term fuck-quic {
                from {
                    next-header udp;
                    destination-port [ http https ];
                }
                then {
                    reject;
                }
            }
            term default {
                then accept;
            }
        }
    }
}
