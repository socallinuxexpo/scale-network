	option txpower '8'
	option country 'US'


config wifi-iface 'scaleslow_radio0'
	option device 'radio0'
        option ssid 'scale-public-slow'
	option mode 'ap'
	option network 'mgmt'
	option encryption 'psk2'
	option key 'pasadena'
	option disabled '0'

